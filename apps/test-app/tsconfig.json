{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"allowJs": true,
		"strict": true,
		"noEmit": true,
		"types": [
			"vinxi/client"
		],
		"isolatedModules": true,
		"paths": {
			"~/*": [
				"./src/*"
			]
		}
	}
}